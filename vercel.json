{
  "version": 2,
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/templates/(.*)",
      "dest": "/templates/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "builds": [
    {
      "src": "/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "/templates/**",
      "use": "@vercel/node"
    }
  ]
}
