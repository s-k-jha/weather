{
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "builds": [
    {
      "src": "/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "/templates/**",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/app.js": {
      "runtime": "nodejs16.x",
      "memory": 512
    }
  }
}
